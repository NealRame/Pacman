{
  "name": "maze-of-shadow",
  "version": "0.0.1",
  "description": "Generate random mazes.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NealRame/maze-of-shadow.git"
  },
  "keywords": [
    "Maze"
  ],
  "author": "Neal.Rame. <contact@nealrame.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NealRame/maze-of-shadow/issues"
  },
  "homepage": "https://github.com/NealRame/maze-of-shadow#readme",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-runtime": "^5.8.25",
    "babelify": "^6.1.3",
    "browserify": "^11.2.0",
    "browsernizr": "^1.1.0",
    "chalk": "^1.1.0",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "underscore": "^1.8.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": "runtime"
        }
      ]
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "camelcase": 0,
      "curly": [
        1,
        "multi-line"
      ],
      "key-spacing": 0,
      "new-cap": [
        1,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "no-multi-spaces": 0,
      "quotes": [
        1,
        "single"
      ],
      "space-infix-ops": 0,
      "strict": [
        2,
        "never"
      ]
    }
  }
}
