{
  "name": "maze-of-shadow",
  "version": "0.0.1",
  "description": "Generate random mazes.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NealRame/maze-of-shadow.git"
  },
  "keywords": [
    "Maze"
  ],
  "author": "Neal.Rame. <contact@nealrame.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NealRame/maze-of-shadow/issues"
  },
  "homepage": "https://github.com/NealRame/maze-of-shadow#readme",
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "babel": "^5.8.21",
    "babelify": "^6.1.3"
  },
  "browserify": {
    "transform": [
      ["babelify", {"optional": "runtime"}]
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "camelcase": 0,
      "curly": [1, "multi-line"],
      "key-spacing": 0,
      "new-cap": [
        1,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "no-multi-spaces": 0,
      "quotes": [1, "single" ],
      "space-infix-ops": 0,
      "strict": [2, "never"]
    }
  }
}
